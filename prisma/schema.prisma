generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model File {
  id          Int      @id @default(autoincrement())
  key         String   @unique
  filename    String
  contentType String?
  size        Int
  url         String?
  createdAt   DateTime @default(now())
}